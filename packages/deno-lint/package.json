{
  "name": "@node-rs/deno-lint",
  "version": "0.6.0",
  "description": "Deno lint binding for NodeJS",
  "keywords": ["Deno", "Lint", "ESLint", "node-rs", "napi", "N-API", "Rust", "napi-rs"],
  "author": "LongYinan <lynweklm@gmail.com>",
  "homepage": "https://github.com/napi-rs/node-rs",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "denolint": "./bin.js"
  },
  "typings": "index.d.ts",
  "files": ["index.js", "index.d.ts", "bin.js", "webpack-loader.js", "LICENSE"],
  "napi": {
    "name": "deno-lint",
    "triples": {
      "defaults": true,
      "additional": ["x86_64-unknown-linux-musl", "aarch64-unknown-linux-gnu", "aarch64-apple-darwin"]
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/napi-rs/node-rs.git"
  },
  "scripts": {
    "artifacts": "napi artifacts -d ../../artifacts",
    "bench": "cross-env NODE_ENV=production node benchmark/lint.js",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish",
    "version": "napi version"
  },
  "bugs": {
    "url": "https://github.com/napi-rs/node-rs/issues"
  },
  "dependencies": {
    "@node-rs/helper": "^1.0.0-alpha.1",
    "loader-utils": "^2.0.0"
  },
  "devDependencies": {
    "@types/webpack": "^4.41.25"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Brooooooklyn"
  }
}
