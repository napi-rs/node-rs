{
  "name": "@node-rs/deno-lint",
  "version": "1.10.2",
  "description": "Deno lint binding for Node.js",
  "keywords": ["Deno", "Lint", "ESLint", "node-rs", "napi", "N-API", "Rust", "napi-rs"],
  "author": "LongYinan <lynweklm@gmail.com>",
  "homepage": "https://github.com/napi-rs/node-rs",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "denolint": "./bin.js"
  },
  "typings": "index.d.ts",
  "files": ["index.js", "index.d.ts", "bin.js", "cli.js", "webpack-loader.js", "LICENSE"],
  "napi": {
    "name": "deno-lint",
    "triples": {
      "defaults": true,
      "additional": [
        "i686-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "x86_64-unknown-freebsd",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/napi-rs/node-rs.git"
  },
  "scripts": {
    "artifacts": "napi artifacts -d ../../artifacts",
    "bench": "cross-env NODE_ENV=production node benchmark/lint.js",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish",
    "version": "napi version"
  },
  "bugs": {
    "url": "https://github.com/napi-rs/node-rs/issues"
  },
  "dependencies": {
    "@node-rs/helper": "^1.2.1",
    "clipanion": "^3.1.0",
    "loader-utils": "^3.1.0",
    "typanion": "^3.7.1"
  },
  "devDependencies": {
    "@types/webpack": "^5.28.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Brooooooklyn"
  }
}
